	void setCod(int cod) {
		this.cod = cod;
	}
	
	static int getCod() {
		return cod;
	}

	void setNome(String nome) {
		this.nome = nome;
	}
	
	static String getNome() {
		return nome;
	}
	
	void setAutor(String autor) {
		this.autor = autor;
	}
	
	static String getAutor() {
		return autor;
	}
	
	void setGenero(String genero) {
		this.genero = genero;
	}
	
	public String getGenero() {
		return genero;
	}	
	
